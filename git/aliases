[alias]
	tree= log --graph --all --date-order --left-right --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	t= log --graph --all --date-order --left-right --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	tree-branch= log --graph --date-order --left-right --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	tb= log --graph --date-order --left-right --pretty=format:'%C(auto)%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	c= commit -m
	cc= commit
	c-= commit --amend
	commit-with-add= !git add -A . && git commit -m a
	ca= !git add -A. && git commit -m a
	cf= clean -df
	a= add
	addpick= add -p
	ap= add -p
	addall= add -A .
	aa= add -A .
	o= checkout
	checkoutforce= checkout -f
	of= checkout -f
	ob= checkout HEAD^
	r= reset
	rp= reset -p
	resethard= reset --hard
	rh= reset --hard
	resetback= reset HEAD^
	rb= reset HEAD^
	rbb= reset HEAD^^
	rbbb= reset HEAD^^^
	rbbbb= reset HEAD^^^^
	rbbbbb= reset HEAD^^^^^
	rbbbbbb= reset HEAD^^^^^^
	rbbbbbbb= reset HEAD^^^^^^^
	r-= reset HEAD@{1}
	r--= reset HEAD@{2}
	r---= reset HEAD@{3}
	r----= reset HEAD@{4}
	r-----= reset HEAD@{5}
	r------= reset HEAD@{6}
	r-------= reset HEAD@{7}
	resetbackhard= reset --hard HEAD^
	rbh= reset --hard HEAD^
	m= merge --no-ff
	mff= merge --ff-only
	checkoutmerge= checkout --merge
	om= checkout --merge
	b= branch
	branchall= branch -a
	ba= branch -a
	branchmove= branch -m
	branchmoveforce= branch -M
	branchrename= branch -m
	branchrenameforce= branch -M
	bm= branch -m
	bmf= branch -M
	branchdelete= branch -d
	bd= branch -d
	branchdeleteforce= branch -D
	bdf= branch -D
	branchcreate= checkout -b
	bc= checkout -b
	branchcreateforce= checkout -B
	bcf= checkout -B
	bf= branch -f
	branchmergecreate= checkout --merge -b
	bmc= checkout --merge -b
	branchmergecreateforce= checkout --merge -B
	bmcf= checkout --merge -B
	p= push
	pa= push --all
	pushforce= push -f
	pf= push -f
	l= pull
	la= pull --all
	f= fetch
	fa= fetch --all
	fetchprune= fetch --prune
	fp= fetch --prune
	fap= fetch --all --prune
	s= status
	d= diff
	diffback= diff HEAD^
	db= diff HEAD^
	cp= cherry-pick
	ignore= update-index --assume-unchanged
	i= update-index --assume-unchanged
	unignore= update-index --no-assume-unchanged
	ui= update-index --no-assume-unchanged
	rv= remote -v
